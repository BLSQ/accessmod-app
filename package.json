{
  "name": "accessmod-app",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev:watch": "npm-run-all --parallel dev 'codegen -- --watch'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky install",
    "schema": "npx get-graphql-schema http://localhost:8000/graphql/ > schema.graphql",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "graphql": {
    "schema": "./schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/uuid": "^8.3.4",
    "clsx": "^1.1.1",
    "deepmerge": "^4.2.2",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^15.0.0",
    "husky": "^7.0.4",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.isequal": "^4.5.0",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-select": "^5.2.2",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.3",
    "@graphql-codegen/typescript-react-apollo": "3.2.4",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.1",
    "babel-jest": "^27.4.5",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.8"
  }
}
