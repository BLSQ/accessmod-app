{
  "name": "accessmod-app",
  "private": true,
  "version": "0.12.0",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "npm-run-all --parallel next 'codegen -- --watch'",
    "next": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint --dir src",
    "format": "prettier --write src",
    "prepare": "husky install",
    "schema": "npx --yes get-graphql-schema http://localhost:8000/graphql/ > schema.graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "i18n:extract": "i18next"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "graphql": {
    "schema": "./schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@dnd-kit/sortable": "^7.0.1",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@popperjs/core": "^2.11.6",
    "@react-leaflet/core": "^2.1.0",
    "@sentry/nextjs": "^7.16.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/uuid": "^8.3.4",
    "axios": "^1.1.3",
    "chroma-js": "^2.4.2",
    "clsx": "^1.2.1",
    "csv-parse": "^5.3.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "filesize": "^10.0.5",
    "geojson-vt": "^3.2.1",
    "georaster": "^1.5.6",
    "georaster-layer-for-leaflet": "^3.10.0",
    "geotiff": "^2.0.5",
    "graphql": "^16.6.0",
    "husky": "^8.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "jest-environment-jsdom": "^29.2.2",
    "leaflet": "^1.9.2",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "leaflet-geotiff-2": "^1.1.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "luxon": "^3.0.4",
    "next": "^12",
    "next-i18next": "^12.1.0",
    "nextjs-progressbar": "^0.0.16",
    "plotty": "^0.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-leaflet": "^4.1.0",
    "react-popper": "^2.3.0",
    "react-popper-tooltip": "^4.4.2",
    "react-select": "^5.5.7",
    "sharp": "^0.31.1",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
