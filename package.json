{
  "name": "accessmod-app",
  "private": true,
  "version": "0.11.1",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "watch": "npm-run-all --parallel dev 'codegen -- --watch'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint --dir src",
    "format": "prettier --write src",
    "prepare": "husky install",
    "schema": "npx get-graphql-schema http://localhost:8000/graphql/ > schema.graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "i18n:extract": "i18next"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "graphql": {
    "schema": "./schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.6.1",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@popperjs/core": "^2.11.5",
    "@react-leaflet/core": "^1.0.0",
    "@sentry/nextjs": "^6.19.7",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^17.0.31",
    "@types/react": "^17.0.39",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "chroma-js": "^2.4.2",
    "clsx": "^1.1.1",
    "csv-parse": "^5.0.4",
    "deepmerge": "^4.2.2",
    "dropzone": "^6.0.0-beta.2",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "filesize": "^8.0.7",
    "geojson-vt": "^3.2.1",
    "georaster": "^1.5.6",
    "georaster-layer-for-leaflet": "^3.8.0",
    "graphql": "^16.4.0",
    "husky": "^7.0.4",
    "isomorphic-unfetch": "^3.1.0",
    "jest-environment-jsdom": "^28.0.2",
    "leaflet": "^1.8.0",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "leaflet-geotiff-2": "^1.1.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "luxon": "^2.3.2",
    "next": "^12.1.5",
    "next-i18next": "^11.0.0",
    "nextjs-progressbar": "^0.0.14",
    "plotty": "^0.4.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^14.1.2",
    "react-leaflet": "^3.0.0",
    "react-popper": "^2.3.0",
    "react-popper-tooltip": "^4.3.1",
    "react-select": "^5.3.1",
    "sharp": "^0.30.4",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
