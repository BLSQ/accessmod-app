    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^14.1.1",
    "@types/chroma-js": "^2.1.3",
    "@types/leaflet": "^1.7.9",
    "@types/lodash.merge": "^4.6.7",
    "@types/luxon": "^2.3.1",
    "@types/react-table": "^7.7.11",
    "autoprefixer": "10.4.5",
    "babel-jest": "^28.0.3",
    "i18next-parser": "^6.4.0",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "react-table": "^7.7.0",
    "tailwindcss": "^3.0.24"
  }
