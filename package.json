    "@types/chroma-js": "^2.1.4",
    "@types/leaflet": "^1.9.0",
    "@types/lodash.merge": "^4.6.7",
    "@types/luxon": "^3.0.2",
    "@types/react-table": "^7.7.12",
    "autoprefixer": "10.4.12",
    "babel-jest": "^29.2.2",
    "i18next-parser": "^6.6.0",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-table": "^7.8.0",
    "tailwindcss": "^3.2.1"
